<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_weight="1"
    android:layout_marginTop="18dp"
    android:gravity="center_vertical"
    android:layout_height="0dp">


    <TextView
        android:layout_marginTop="12dp"
        android:text="4"
        android:id="@+id/paihang_shengyu_zimu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <ImageView
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@+id/paihang_shengyu_zimu"
        android:id="@+id/paihang_sy_touxiang"
        android:src="@mipmap/ic_launcher"
        android:layout_width="42dp"
        android:layout_height="42dp" />

    <TextView
        android:layout_marginLeft="17dp"
        android:layout_marginTop="12dp"
        android:layout_toRightOf="@+id/paihang_sy_touxiang"
        android:text="@string/yonghuiming"
        android:id="@+id/paihang_sy_ageandsex"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:layout_marginTop="14dp"
        android:paddingRight="10dp"
        android:paddingLeft="10dp"
        android:background="@drawable/tv_find_bg"
        android:drawablePadding="3dp"
        android:drawableLeft="@drawable/miss"
        android:textSize="11sp"
        android:layout_marginLeft="7dp"
        android:layout_toRightOf="@+id/paihang_sy_ageandsex"
        android:text="22"
        android:id="@+id/find_zuixin_age"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />



    <TextView
        android:drawablePadding="2dp"
        android:drawableLeft="@drawable/like_red"
        android:layout_marginTop="14dp"
        android:textColor="@color/colorPrimary"
        android:layout_marginRight="3dp"
        android:layout_alignParentRight="true"
        android:text="12000"
        android:id="@+id/paihang_sy_money"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</RelativeLayout>
